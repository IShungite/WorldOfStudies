{
  "name": "@world-of-studies/api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --watch",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "migration:run": "node ace migration:run",
    "test:mutation": "stryker run"
  },
  "imports": {
    "#controllers/*": "./app/adapters/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js",
    "#factories/*": "./app/domain/factories/*.js",
    "#domainServices/*": "./app/domain/services/*.js",
    "#domainModels/*": "./app/domain/models/*.js",
    "#domainPorts/*": "./app/domain/ports/*.js",
    "#repositories/*": "./app/adapters/repositories/*.js",
    "#mappers/*": "./app/adapters/mappers/*.js",
    "#utils/*": "./utils/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.2.3",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@japa/api-client": "^2.0.2",
    "@japa/assert": "^2.1.0",
    "@japa/plugin-adonisjs": "^3.0.0",
    "@japa/runner": "^3.1.1",
    "@stryker-mutator/core": "^8.2.6",
    "@swc/core": "^1.4.8",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.28",
    "eslint": "^8.57.0",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.1.1",
    "@adonisjs/core": "^6.3.1",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/lucid": "^20.4.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@vinejs/vine": "^1.8.0",
    "better-sqlite3": "^9.4.3",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.4",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1"
  },
  "prettier": "@adonisjs/prettier-config"
}
